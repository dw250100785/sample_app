<li>
  <%= link_to user.name, user %>
  <=>
  <%= link_to user.email, user %>

  <% if current_user.admin? and !current_user?(user) %>
    <=> <%= link_to 'delete', user, method: :delete, confirm: 'DELETE USER?' %>
  <% end %>

  <% if signed_in? and !current_user?(user) %>
    <%= render 'show_follow', object: user %>
  <% end %>
</li>